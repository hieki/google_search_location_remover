{
  "name": "Google Search Location Remover",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Remove Google Search location",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["remover.js"]
    }
  ]
}
